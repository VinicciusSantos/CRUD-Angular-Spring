{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8d3ce4bc",
    "activityBar.background": "#9b3ce436",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#bf8313",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#8d3ce4bc",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#8d3ce4bc",
    "statusBarItem.remoteBackground": "#8d3ce4bc",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#9b3ce436",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#9b3ce436",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#5E1616"
